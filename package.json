    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.24"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
