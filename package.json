    "@testing-library/user-event": "^13.5.0",
    "@walletconnect/web3-provider": "^1.7.8",
    "assert": "^2.0.0",
    "big.js": "^6.1.1",
    "classnames": "^2.3.1",
    "crypto-browserify": "^3.12.0",
    "ethers": "^5.6.4",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "react": "^18.1.0",
    "react-app-rewired": "^2.2.1",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.1.0",
